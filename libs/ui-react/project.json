{
  "name": "@ldls/ui-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui-react/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:ui"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/ui-react/dist"
      },
      "dependsOn": ["generate-symbols"]
    },
    "generate-symbols": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/ui-react/src/tools/generate-symbols.ts"
      }
    }
  }
}
