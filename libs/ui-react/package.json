{
  "name": "@ledgerhq/ldls-ui-react",
  "version": "0.0.30",
  "license": "Apache-2.0",
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "tailwind",
    "radix-ui"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./symbols": {
      "types": "./dist/lib/Symbols/index.d.ts",
      "import": "./dist/lib/Symbols/index.js",
      "default": "./dist/lib/Symbols/index.js"
    },
    "./*": "./dist/lib/Components/*/index.js"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "tags": [
      "scope:react",
      "type:ui"
    ]
  },
  "dependencies": {
    "@ledgerhq/ldls-utils-shared": "0.0.6"
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwind-merge": "^2.6.0"
  }
}
