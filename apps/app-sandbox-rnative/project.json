{
  "name": "app-sandbox-rnative",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-sandbox-rnative/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo start --dev-client",
        "cwd": "apps/app-sandbox-rnative"
      }
    },
    "ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo run:ios",
        "cwd": "apps/app-sandbox-rnative"
      }
    },
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo prebuild --platform ios",
        "cwd": "apps/app-sandbox-rnative"
      }
    },
    "prebuild-clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo prebuild --clean --platform ios",
        "cwd": "apps/app-sandbox-rnative"
      }
    }
  },
  "tags": ["scope:react-native", "type:ui"]
}
