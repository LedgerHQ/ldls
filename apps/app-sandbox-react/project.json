{
  "name": "app-sandbox-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/app-sandbox-react/src",
  "targets": {
    "test:tree-shaking": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/tests/treeShaking/dist"],
      "options": {
        "commands": [
          "vite build --config tests/treeShaking/vite.treeshaking.config.ts",
          "node tests/treeShaking/verify-bundle.mjs"
        ],
        "cwd": "apps/app-sandbox-react",
        "parallel": false
      },
      "dependsOn": ["^build"]
    }
  },
  "tags": ["scope:react"]
}

